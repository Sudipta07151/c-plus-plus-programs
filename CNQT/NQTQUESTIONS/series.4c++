#include <iostream>
using namespace std;
int main()
{
    int odd=0,even=0,term;
    cin>>term;
    for(int i=1;i<=term;i++)
    {
        if(i==1 || i==2)
        {
            cout<<0<<" ";
        }
        else if(i%2!=0)
        {
            odd+=7;
            cout<<odd<<" ";
        }
        else
        {
            even+=6;
            cout<<even<<" ";
        }
    }
    return 0;
}
